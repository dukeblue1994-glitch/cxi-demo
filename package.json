{
  "name": "cxi-demo",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "packageManager": "npm@10.8.1",
  "scripts": {
    "clean": "rm -rf dist && mkdir -p dist/assets",
    "build": "npm run clean && node scripts/build.mjs",
    "dev": "npm run build && npx http-server dist -p 8888 -c-1",
    "test:smoke": "playwright test -g '@smoke'"
  },
  "dependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@playwright/test": "1.47.2",
    "@types/node": "20.14.11",
    "@types/react": "18.3.5",
    "@types/react-dom": "18.3.0",
    "esbuild": "^0.25.11",
    "netlify-cli": "^23.9.1",
    "typescript": "5.5.4"
  }
}
